{
  "name": "microapp-react-temp",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "cz": "git-cz",
    "format": "prettier --write .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "pnpm run lint:script && pnpm run lint:style && pnpm run lint:cspell && pretty-quick --check --branch dev",
    "lint:fix": "pnpm run lint:script && pnpm run lint:style && pnpm run lint:cspell && pretty-quick --branch dev",
    "lint:cspell": "cspell **",
    "lint:script": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:style": "stylelint 'src/**/*.{css,scss,less}' --fix",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm -y"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*": [
      "pnpm run lint:cspell"
    ],
    "*.{js,jsx,ts,tsx}": [
      "pnpm run lint:script"
    ],
    "*.{css,scss,less}": [
      "pnpm run lint:style"
    ]
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.2.3",
    "ahooks": "^3.7.1",
    "antd": "^4.23.5",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "connected-react-router": "^6.9.3",
    "echarts": "^5.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qiankun": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "reselect": "^4.1.6",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "^4.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^6.12.0",
    "customize-cra": "^1.0.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4"
  }
}
