{
  "name": "microapp-react-temp",
  "version": "1.0.0",
  "description": "react microapp spa",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "analyzer": "ANALYZER=true craco build",
    "test": "craco test",
    "commit": "git-cz",
    "format": "prettier --write .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:fix": "npm run lint:script && npm run lint:style && pretty-quick --check",
    "lint:script": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:style": "stylelint 'src/**/*.{css,scss,less}' --fix",
    "lint:cspell": "cspell **",
    "lang:extract": "formatjs extract \"src/**/!(*.d).{js,jsx,ts,tsx}\" --out-file src/locale/extract/zh-CN.json",
    "lang:compile": "formatjs compile-folder src/locale/extract src/locale/compile",
    "lang": "npm run lang:extract && npm run lang:compile",
    "exportNginx": "node scripts/export-nginx.js",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:script"
    ],
    "*.{css,scss,less}": [
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@codemirror/commands": "^6.2.2",
    "@codemirror/lang-javascript": "^6.1.5",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.9.3",
    "@redux-devtools/extension": "^3.2.5",
    "ahooks": "^3.7.2",
    "antd": "^5.8.5",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.0",
    "fabric": "^5.3.0",
    "file-saver": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "qiankun": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-intl": "^6.4.4",
    "react-redux": "^8.1.2",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.8",
    "ts-md5": "^1.3.1",
    "typesafe-actions": "^5.1.0",
    "url-join": "^5.0.0",
    "uuid": "^9.0.0",
    "video.js": "^8.5.2",
    "web-vitals": "^3.1.0",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-async-generator-functions": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-proposal-do-expressions": "^7.22.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@craco/craco": "^6.4.5",
    "@formatjs/cli": "^6.1.3",
    "@formatjs/ts-transformer": "^3.13.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/url-join": "^4.0.1",
    "@types/uuid": "^9.0.3",
    "@types/video.js": "^7.3.52",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-plugin-formatjs": "^10.5.3",
    "commitizen": "^4.2.5",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^6.12.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.12.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-formatjs": "^4.10.3",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-loader": "^7.3.3",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-scss": "^4.0.7",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "sass-resources-loader": "^2.2.5",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "5.1.6",
    "webpack-bundle-analyzer": "^4.6.1",
    "worker-loader": "^3.0.8"
  }
}
